# 数据库配置
DB_HOST=localhost
DB_PORT=3306
DB_USER=root
DB_PASSWORD=xw123456
DB_NAME=cutool_db
DB_CHARSET=utf8mb4

# 服务器配置
PORT=3003

# 阿里云配置
ALIYUN_ACCESS_KEY_ID=your_access_key_id
ALIYUN_ACCESS_KEY_SECRET=your_access_key_secret
ALIYUN_ENDPOINT=cloudauth.aliyuncs.com

# 支付宝配置
# 获取方式：参考 ALIPAY_CONFIG_GUIDE.md 文档
# 1. 注册支付宝开放平台账号：https://open.alipay.com/
# 2. 创建应用并获取 APPID
# 3. 生成密钥对（RSA2）
# 4. 上传应用公钥，获取支付宝公钥
ALIPAY_APP_ID=your_app_id

# 私钥格式说明：
# 需要包含完整的 BEGIN 和 END 标记，保留换行符
# 可以使用 \n 表示换行，或者使用实际换行
# 示例格式：
# ALIPAY_APP_PRIVATE_KEY="-----BEGIN RSA PRIVATE KEY-----\nMIIEpAIBAAKCAQEA...\n-----END RSA PRIVATE KEY-----"
ALIPAY_APP_PRIVATE_KEY=your_app_private_key

# 支付宝公钥格式说明：
# 需要包含完整的 BEGIN 和 END 标记
# 可以使用 \n 表示换行，或者使用实际换行
# 示例格式：
# ALIPAY_PUBLIC_KEY="-----BEGIN PUBLIC KEY-----\nMIIBIjANBgkqhkiG...\n-----END PUBLIC KEY-----"
ALIPAY_PUBLIC_KEY=alipay_public_key
ALIPAY_SIGN_TYPE=RSA2
# 沙箱环境设为 True，生产环境设为 False
ALIPAY_DEBUG=False
# 支付完成后跳转的页面
ALIPAY_RETURN_URL=http://localhost:3000/vehicle-verify
# 支付宝异步通知地址（必须是公网可访问的 HTTPS 地址）
ALIPAY_NOTIFY_URL=http://your-domain.com/api/vehicle-verify/alipay/notify

